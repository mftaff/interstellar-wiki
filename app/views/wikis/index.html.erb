<div class="page-header">
    <div class="pull-right">
        <%= link_to "New Wiki", new_wiki_path, class: 'btn btn-success' %>
    </div>
    <h3> Browse the collection.</h3>
</div>

<div class="row">
    <div class="col-md-8">
        <% @wikis.each do |wiki| %>
            <p><%= link_to wiki.title, wiki %></p>
        <% end %>
    </div>
</div>
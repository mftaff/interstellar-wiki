<div class="page-header">
    <% if policy(Wiki).new? %>
        <div class="pull-right">
            <%= link_to "New Wiki", new_wiki_path, class: 'btn btn-success' %>
        </div>
    <% end %>
    <h3> Browse the collection.</h3>
</div>

<div class="row">
    <div class="col-md-8 index-links">
        <%= render partial: 'index', locals: { wikis: @wikis } %>
    </div>
</div>